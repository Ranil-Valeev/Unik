<Window x:Class="Unik2.windows.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Unik2.windows"
        mc:Ignorable="d"
        Title="Admin" Height="500" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Content="Admin Panel" HorizontalAlignment="Center" VerticalAlignment="Center" Width="200" Height="40" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"  FontSize="25"/>
        <Button x:Name="Back" Content="Назад" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="10" Height="30" Width="100" Click="Back_Click" Background="LightCoral" />
        <Frame Grid.Row="1" Name="MainFrame" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
